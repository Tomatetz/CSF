<div class="panel panel-primary">
    <h4>
        <span class="icon-link pull-right details-add-button" data-action="add-<%= title %>"><span class="glyphicon glyphicon-plus font12 colorBlue"></span> <%= buttonTitle %></span>
        <span data-element="collapse-panel-title" class="collapse-btn"
              data-toggle="collapse"
              data-target="[data-id='<%= title %>']"><%= title %> <i class="fa fa-angle-up" ></i></span>
    </h4>
    <div class="in" data-id="<%= title %>">
        <div id="<%= title %>-region"></div>
    </div>
</div>


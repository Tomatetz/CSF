<% if(editable){ %>
<div class="pull-right item-menu-place__glyphContainer">
    <span class="editable-search">editable</span>
    <span class="glyphicon glyphicon-btn glyphicon-pencil"
          data-toggle="tooltip" data-placement="bottom" title="Edit" data-action="edit"></span>

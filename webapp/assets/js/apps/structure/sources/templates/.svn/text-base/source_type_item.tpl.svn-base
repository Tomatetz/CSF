<div class="panel-heading">
    <h4 class="panel-title">
        <a data-toggle="collapse" data-parent=".accordion" href="#collapsable-<%= viewId %>" style="margin-top: 3px; display:block;">
            <span class="badge pull-right"><%= length %></span> <%= title %>
        </a>
    </h4>
</div>
<div id="collapsable-<%= viewId %>" class="panel-collapse collapse">
    <div class="panel-body">
        <div id="source-<%= viewId %>"></div>
    </div>
</div>
.change-view {
  .btn-primary{
    border-color:rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25);
    &.active{
      border-color: #ec8026;
    }
  }
}
.add-new-structure-modal{
  [data-toggle="buttons"]{
    label{
      border-color:rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25);
      margin-right: 1px;
      &.active{
        border-color: #ec8026;
      }
    }
  }
}